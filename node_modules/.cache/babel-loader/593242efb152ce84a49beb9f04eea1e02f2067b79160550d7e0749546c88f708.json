 focus')).then(function (canvas) {\n        var link = document.createElement('a');\n        if (typeof link.download === 'string') {\n          link.href = canvas.toDataURL();\n          link.download = 'aaa.jpeg';\n\n          //Firefox requires the link to be in the body\n          document.body.appendChild(link);\n\n          //simulate click\n          link.click();\n\n          //remove the link when done\n          document.body.removeChild(link);\n        } else {\n          window.open(canvas.toDataURL());\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["groupList","ref","html2canvas","draggable","ContextMenu","ListSaver","VLazyImage","getAuth","onAuthStateChanged","name","order","components","props","data","homePageArrays","testArray","editModeOn","showContextMenu","showContextMenuCat","showListSaver","clickedPerson","clickedCat","clickedIndex","groups","currUser","fireSaveData","unsortedCollapsed","mounted","_this","auth","user","initialize","methods","_this2","_asyncToGenerator","_regeneratorRuntime","mark","_callee","currentUser","userDoc","saveData","i","_i","wrap","_callee$","_context","prev","next","console","log","$db","collection","doc","uid","get","sent","categories","length","push","people","stop","addNewCategory","arr","set","toggleEditModeOn","toggleEditModeOff","window","location","reload","updateCatOrder","updateStorage","array","updateCatName","catName","openContextMenu","e","person","$refs","menu","open","openContextMenuCat","category","index","menuCat","openListSaver","listSaver","closeListSaver","close","removeFromHome","_this3","j","imgLink","filter","p","removeFromHomeCat","moveToUnsorted","_this4","cat","populateGroupPage","grpName","selectedGroup","groupName","localStorage","setItem","JSON","stringify","$router","document","body","scrollTop","documentElement","saveJSON","a","createElement","file","Blob","type","href","URL","createObjectURL","download","click","showFileUploader","uploadFile","_this5","getElementById"